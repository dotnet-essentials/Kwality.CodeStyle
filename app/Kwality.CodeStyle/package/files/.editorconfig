csharp_place_simple_blocks_on_single_line = false                         # - Place simple blocks on single line.
csharp_place_simple_declaration_blocks_on_single_line = false             # - Place blocks from simple type members on single line.
csharp_place_attribute_on_same_line = false                               # - Place simple attribute on the same line with the owner.
csharp_space_after_unary_operator = false                                 # - Space after unary operators (!, -, ~,...).
csharp_space_after_comma = true                                           # - Space after comma.
csharp_space_around_ternary_operator = false                              # - Space around ternary operators (?, :).
csharp_space_around_binary_operator = false                               # - Space around binary operators (+, ||, =, ...).
csharp_space_around_member_access_operator = false                        # - Space around member access operators (., ->, ...).
csharp_space_before_open_square_brackets = false                          # - Space before brackets.
