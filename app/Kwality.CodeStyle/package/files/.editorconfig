csharp_space_between_square_brackets = false                              # - Space within brackets.
csharp_space_between_parentheses_of_control_flow_statements = false       # - Space within parenthesis of control flow statements.
configure_await_analysis_mode = library                                   # - ConfigureAwait analysis mode.

